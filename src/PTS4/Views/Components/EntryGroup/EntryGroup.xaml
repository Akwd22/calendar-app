<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:components="clr-namespace:PTS4.Views.Components"
             x:Class="PTS4.Views.Components.EntryGroup">
    <ContentView.Content>
        <StackLayout>
            <components:CalendarHeader x:Name="CalendarHeader" List="{x:Reference EntryList}" />
            <StackLayout x:Name="EntryList" Margin="0, 5, 0, 10">
            </StackLayout>
        </StackLayout>
    </ContentView.Content>
</ContentView>